<%-include('../partition/header.ejs')%>
<br><br>
<section class="login-wrap mtb-40">
<div class="container">
<div class=" justify-content-center">
<div class="corowl-md-6">
<div class="login-box">
<h1 class="alert alert-success text-center">Student Record</h1>
<%# Show All Data %>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            
            <th>NAME</th>
            <th>EMAIL</th>
            <th>PHONE</th>
            <th>ADDRESS</th>
            <th>COLLEGE</th>
            <th>COURSE</th>
            <th>STATUS</th>
            <th>ACTION</th>
        </tr>
    </thead>
    <tbody>
        
        <% data1.forEach((item)=>{ %>

            <tr>
                
                <td>
                    <%= item.name %>
                </td>
                <td>
                    <%= item.mail %>
                </td>
                <td>
                    <%= item.phone %>
                </td>
                <td>
                    <%= item.address %>
                </td>
                <td>
                    <%= item.college %>
                </td>
                <td>
                    <%= item.course %>
                </td>
 
                <td>
                    <button type="button" class="btn btn-outline-success"><%= item.status %></button>       
                </td>
                
                <td>
                    
                    <a class="btn btn-outline-primary" href="/admin_college/edit/<%= item._id %>">Edit </a><br><br>
                    <a class="btn btn-outline-danger" href="/admin_college/delete/<%= item._id %>">delete </a>

                </td>
            </tr>
            <%})%>  

                
                
    </tbody>
</table>


</div>
</div>
</div>
</div>
</section>
<%-include('../partition/footer.ejs')%>