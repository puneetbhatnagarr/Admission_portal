            <input type="number" name="p" placeholder="Enter mobile number" class="form-control">
            <label style="color: black;">Mobile number should be encrypted</label>
            </div>
            <div class="mb-3">
            <label style="color: black;">Address</label>
            <textarea class="form-control" name="a" rows="3"></textarea>
            </div>
            <div class="mb-3">
            <label style="color: black;">Sex</label><br>
            <input type="radio" name="g" value="male"> <span style="color: black;" >Male</span>
            <input type="radio" name="g" value="female"><span style="color: black;" >Female</span>
            <input type="radio" name="g" value="others"><span style="color: black;" >Others</span>
            </div>
            
            <div class="mb-3">
            <label style="color: black;">College</label> 
            <br>
            <select name="clg" class="form-control" required>
            <option value="0">Select</option>
            <option value="RJIT">RJIT</option>
            <option value="MITS">MITS</option>
            <option value="ITM">ITM</option>
            <option value="AVB-IIITM">AVB-IIITM</option>
            <option value="JIWAJI UNIVERSITY">JIWAJI UNIVERSITY</option>
            <option value="OTHERS">OTHERS</option>
            
            </select>
            </div>
            
            <div class="mb-3">
            <label style="color: black;">Course</label>
            <br>
            <select name="course" class="form-control" required>
            <option value="0">Select</option>
            <option value="B.TECH">B.TECH</option>
            <option value="BSC">BSC</option>
            <option value="BBA">BBA</option>
            <option value="B.COM">B.COM</option>
            
            <option value="M.TECH">M.TECH</option>
            <option value="MSC">MSC</option>
            <option value="M.COM">M.COM</option>                                                                   
            <option value="MBA">MBA</option>
            <option value="OTHER">OTHER</option>
                                            
            
            </select>
            </div>                                            
            <div>
            <input type="hidden" name="user_id" value="<%=data._id %>" >
            <button class="btn btn-secondary btn-block border-0 py-3"  type="submit">SUBMIT</button>
            </div>

         
             
            </form>
